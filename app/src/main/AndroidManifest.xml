<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe.TransparentTheme">
        <activity android:name=".activity.OnlineGameActivity"></activity>
        <activity android:name=".activity.JoinGameActivity" />
        <activity android:name=".activity.PlayVsFriendActivity" />
        <activity android:name=".activity.PlayOnlineActivity" />
        <activity android:name=".activity.PlaySoloActivity" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".activity.SignupActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>